	private String EJ2;
	private String EJ3;
	private String EJ4;
	private String EJ5;
	private String EJ6;
	private String EJ7;


	
	public Todo() {
EJ1 = "CREATE TABLE fabricantes ("
		+ "ID INT PRIMARY KEY, nombre VARCHAR(100) NOT NULL);";

	}
	
	public static String getEJ1() {
		
		return EJ1;
	}

	public String getEJ2() {
		return EJ2;
	}

